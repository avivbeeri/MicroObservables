window.observable=function(a){var b={};return a.on=function(c,d){return"function"==typeof d?(b[c]||(b[c]=[]),b[c].push(d),a):void 0},a.off=function(c){c=c.split(/\s+/);for(var d=0;d<c.length;d++)delete b[c[d]];return a},a.trigger=function(c){var d=Array.prototype.slice.call(arguments,1);fns=b[c]||[];for(var e=0;e<fns.length;e++)fns[e].apply(fns[e],d);return a},a},"function"==typeof window.define&&window.define.amd&&window.define("micorObservables",[],function(){return window.observable});